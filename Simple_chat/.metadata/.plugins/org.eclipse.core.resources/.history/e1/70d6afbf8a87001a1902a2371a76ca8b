package SWmaestro;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class SW02 {

	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		String[] input = br.readLine().split(" ");

		int n = Integer.parseInt(input[0]);
		int m = Integer.parseInt(input[1]);

		int[][] stat = new int[n][2];
		int[][] relation = new int[m][3];
		// relation[m][2] 은 team Number로 지정
		// relation[m][0], relation[m][1]는 관계 지정

		for (int i = 0; i < n; i++) {
			input = br.readLine().split(" ");
			for (int j = 0; j < 2; j++) {
				stat[i][j] = Integer.parseInt(input[j]);
			}
		}
		for (int i = 0; i < n; i++) {
			input = br.readLine().split(" ");
			relation[i][2]=i;
			for (int j = 0; j < 2; j++) {
				relation[i][j] = Integer.parseInt(input[j]);
				
			}
		}
		
		
		
		for(int i=0;i<n;i++) {
			for(int j=0;j<n;j++) {
				for(int a=0;a<2;a++) {
					for(int b=0;b<2;b++) {
						if(relation[i][a]==relation[j][b]) {
							relation[j][2]=relation[i][2];
						}
					}
				}
			}
		}

		for(int i=0;i<n;i++) {
			for(int j=0;j<3;j++) {
				
			}
		}

	}

}
