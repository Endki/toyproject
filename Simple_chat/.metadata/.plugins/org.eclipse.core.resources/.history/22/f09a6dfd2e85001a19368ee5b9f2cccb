						break;
				case 1: sum+=(gandalf[i]*2);
						break;
				case 4: sum+=(gandalf[i]*4);
						break;
				case 5: sum+=(gandalf[i]*10);
				break;
				default:
				}
				
//				if(i==1) {
//					
//				}else if(i==0) {
//					
//				}else if(i==4) {
//					
//				}else if(i==5) {
//					
//				}else {
//					
//				}
			}
			k=br.readLine().split(" ");
			for(int i=0;i<7;i++) {
				sauron[i]=Integer.parseInt(k[i]);
			}
			
			
			
			
			
			
			
			
			
			
			
			System.out.print("Battle "+count+": ");
